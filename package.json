{
	"name": "img-hasher",
	"description": "A fast image hash generator and hamming distance calculator using multiple algorithms",
	"version": "0.0.2",
	"main": "index.js",
	"types": "index.d.ts",
	"napi": {
		"name": "img-hasher",
		"package": {
			"name": "@imranbarbhuiya/img-hasher"
		},
		"triples": {
			"additional": [
				"aarch64-apple-darwin",
				"x86_64-unknown-linux-musl"
			]
		}
	},
	"license": "MIT",
	"scripts": {
		"artifacts": "napi artifacts",
		"build": "napi build --platform --release",
		"build:debug": "napi build --platform",
		"prepublishOnly": "napi prepublish -t npm",
		"universal": "napi universal",
		"version": "napi version"
	},
	"devDependencies": {
		"@napi-rs/cli": "^2.15.2",
		"@types/node": "^20.1.4",
		"eslint": "^8.40.0",
		"eslint-config-mahir": "^0.0.26",
		"prettier": "^2.8.8"
	},
	"files": [
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"hash",
		"image hash",
		"phash",
		"dhash",
		"hamming distance",
		"image difference",
		"image distance",
		"image similarity",
		"perceptual hash",
		"perceptual image hash"
	],
	"engines": {
		"node": ">= 10"
	},
	"packageManager": "yarn@3.5.1",
	"repository": "https://github.com/imranbarbhuiya/image-distance"
}
